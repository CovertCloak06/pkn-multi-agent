# Parakleon/Divine Node Environment Variables Template
# Copy this file to .env and add your API keys

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# HuggingFace API Configuration
HUGGINGFACE_API_KEY=your_huggingface_token_here
HUGGINGFACE_MODEL=meta-llama/Meta-Llama-3-8B-Instruct

# Groq API Configuration (optional - free tier available)
GROQ_API_KEY=
GROQ_MODEL=llama-3.3-70b-versatile

# Together API Configuration (optional)
TOGETHER_API_KEY=
TOGETHER_MODEL=meta-llama/Llama-3-70b-chat-hf

# Local Server Configuration (defaults - usually no need to change)
OLLAMA_BASE_URL=http://127.0.0.1:11434/v1
LLAMACPP_BASE_URL=http://127.0.0.1:8000/v1
